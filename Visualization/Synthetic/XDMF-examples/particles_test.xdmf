<?xml version="1.0" ?>
<Xdmf Version="2.0" xmlns:xi="http://www.w3.org/2001/XInclude">
  <Domain Name="domain">
    <Grid GridType="Uniform" Name="velocity">
      <Topology Dimensions="200" Name="topology" TopologyType="Polyvertex"/>
      <Geometry GeometryType="XYZ" Name="geometry">
        <DataItem DataType="Float" Dimensions="200 3" Format="HDF" Precision="8">test.h5:/particles/position</DataItem>
      </Geometry>
      <Attribute Center="Node" Name="mass">
        <DataItem DataType="Float" Dimensions="200" Format="HDF" Precision="8">test.h5:/particles/mass</DataItem>
      </Attribute>
      <Attribute AttributeType="Vector" Center="Node" Name="velocity">
        <DataItem DataType="Float" Dimensions="200 3" Format="HDF" Precision="8">test.h5:/particles/velocity</DataItem>
      </Attribute>
    </Grid>
  </Domain>
</Xdmf>
