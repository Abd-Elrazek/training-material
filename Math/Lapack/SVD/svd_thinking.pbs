#!/bin/bash -l
#PBS -l nodes=1:ppn=20:ivybridge
#PBS -l mem=120gb
#PBS -l walltime=4:00:00
#PBS -m ae

if [ -z "${file}" ]
then
    echo "no file specified" >&2
    exit 1
fi
echo "File: ${file}" >&2

module purge
module load intel/2014a
module load HDF5/1.8.12-intel-2014a-serial

cd $PBS_O_WORKDIR

./svd.exe ${file} matrix >&2
